{
  "agent": {
    "run_as_user": "cwagent"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/logstash/cloudfoundry/dceas-user-site.log",
            "log_group_name": "live",
            "log_stream_name": "dceas-user-site"
          },
          {
            "file_path": "/var/log/logstash/cloudfoundry/dceas-admin-site.log",
            "log_group_name": "live",
            "log_stream_name": "dceas-admin-site"
          },
          {
            "file_path": "/var/log/cloudfoundry/other.log",
            "log_group_name": "live",
            "log_stream_name": "other"
          }
        ]
      }
    }
  },
  "metrics": {
    "metrics_collected": {
      "statsd": {
        "metrics_aggregation_interval": 60,
        "metrics_collection_interval": 10,
        "service_address": ":8125"
      }
    }
  }
}